{
  "root": "libs/efficiency-mappings",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/efficiency-mappings/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/efficiency-mappings"],
      "options": {
        "jestConfig": "libs/efficiency-mappings/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "gen-crds": {
      "executor": "@polaris-sloc/polaris-nx:generate-crds",
      "options": {}
    }
  }
}
